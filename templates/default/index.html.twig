{% extends 'base.html.twig' %}

{% block stylesheet %}
    <link rel="stylesheet" href="{{ asset('assets/default/css/style.css') }}"/>
{% endblock %}

{% block body %}

    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <img src="{{ asset('assets/default/image/jumbotron.png') }}" alt="">
                <h2>Example Jumbotron</h2>
                <div class="btn btn-lg btn-light">
                    <a href="{{ path('jumbotron_home') }}">Jumbotron</a>
                </div>
            </div>
            <div class="col-md-4">
                <img src="{{ asset('assets/default/image/fixed-top-navbar.png') }}" alt="">
                <h2>Example navbar-top-fixed</h2>
                <div class="btn btn-lg btn-light">
                    <a href="{{ path('navbar-top-fixed_home') }}">Fixed top navbar</a>
                </div>
            </div>
            <div class="col-md-4">
                <img src="{{ asset('assets/default/image/imag.png') }}" alt="">
                <h2>Example carousel</h2>
                <div class="btn btn-lg btn-light">
                    <a href="{{ path('carousel_home') }}">Carousel</a>
                </div>
            </div>
        </div>
    </div>

{% endblock %}